       <string notr="true">/* Collapsed Sidebar */
QWidget{
    background-color: rgb(0, 0, 0);  /* Deep black background */
    width: 20px;  /* Narrow width for collapsed state */
    padding: 5px;
    transition: width 0.3s ease-in-out; /* Smooth expansion on hover */
}

/* Expand sidebar on hover */
/*QWidget:hover {
    width: 80px;  /* Wider width on hover (for icons to appear) */
  /*  background-color: rgb(23, 24, 29);  /* Slightly lighter black on hover */


/* Icon button styles */
QPushButton {
    background-color: transparent;
    border: none;
    color: transparent;  /* Hide icon and text in collapsed state */
    font-size: 24px;  /* Icon size */
    height: 40px;
    width: 40px;
    text-align: center;
    transition: background-color 0.2s ease, color 0.2s ease, padding-left 0.2s ease; /* Smooth transition */
}

/* Hover effect on icons */
QPushButton:hover {
    background-color: rgb(33, 34, 39);  /* Darker background when hovered */
    color: #f39c12;  /* Icon color becomes visible on hover (e.g., golden color) */
    padding-left: 10px;  /* Slight padding animation */
}

/* Active (checked) icon button state */
QPushButton:checked {
    background-color: rgb(45, 85, 120);  /* Muted teal/blue background */
    color: #00e5ff;  /* Cyan color for the active icon */
   /* border: 2px solid #00e5ff;  /* Cyan border for active state */
	border-radius: 5px;
    box-shadow: 0px 4px 10px rgb(0, 229, 255);  /* Cyan glow effect */
}

/* Pressed state effect */
QPushButton:pressed {
    background-color: rgb(35, 75, 110);  /* Darker background on press */
}

/* Ensure icons are centered in the collapsed state */
QPushButton::icon {
    color: transparent;  /* Make icons hidden when collapsed */
}

QPushButton:hover::icon {
    color: #f39c12;  /* Make icons visible when hovered */
}
</string>
